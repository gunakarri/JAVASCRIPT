<html>
<head>
	<title>Navigate the url</title>
</head>
<body>
	<div id="output">page 1</div>
	<input type="text" id="url" name="url" value="http://www.google.com">

	<button onclick="goUrl()">click here</button>
	<button onclick="goUrl()">Back</button>
	<button onclick="goUrl()">Forward</button>
	<script>
		var output = document.getElementById('output');
		function goUrl(){
			var url = document.getElementById('url').value;
			window.location = url;
		}
	</script>
</body>
</html>

