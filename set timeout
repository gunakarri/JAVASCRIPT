	<button onclick="startInt()">start</button>
	<button onclick="clearInt()">stop</button>
	<script type="text/javascript">
		var output = document.getElementById('wrapper');

		window.setTimeout(updateWrapper,3330)
		var startRun;

		function startInt(){
			myPopup();
			startRun = window.setTimeout(startInt,13452);
		}

		function updateWrapper(){
			document.getElementById('wrapper').innerHTML = 'welcome to my onboard pages';
		}

		function clearInt(){
			window.clearInterval(startRun)
		}

		function myPopup(){
			prompt('who are you?');
		}
	</script>
